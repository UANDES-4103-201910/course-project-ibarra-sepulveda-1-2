<div class="col-12 page-header text-center">
  	<h1>Dumpsters</h1>
</div>

<div class="col-12"><br></div>
<div class="container" >
	<div class="col-12 text-center">
	  	<%= form_tag "/search/dumpster", method: :get, remote: true, id:"search-form" do %>
	  		<%= text_field_tag :keyword, nil, placeholder: "search"%>
	  		<%= content_tag :button, type: :submit do %>
	  			<i class="fa fa-search"></i>
	  		<% end %>
	  	<% end %>
	</div>
  	<div class="row" id="answer">
    	<%= render @posts %>
    	<%= will_paginate @posts %>
  	</div>
</div>
<div class="col-12">
	<br>
</div>

