<p id="notice"><%= notice %></p>
<div class="container">
	<div class="col-12">
		Name: <%= User.where(id: @blacklist )[0].first_name + User.where(id: @blacklist )[0].last_name %>
	</div>

	<div class="col-12">
		Email: <%= User.where(id: @blacklist )[0].email%>
	</div>

	<div class="col-12">
		Datetime: <%= @blacklist.created_at %>
	</div>	
	<div class="col-12"><br></div>
	<div class="col-12">
		<%= link_to 'Edit', edit_blacklist_path(@blacklist), :class => 'btn btn-info' %>
		<%= link_to 'Back', blacklists_path, :class => 'btn btn-info' %>
	</div>
	<div class="col-12"><br></div>
</div>